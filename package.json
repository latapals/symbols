{
  "name": "@latapals/symbols",
  "version": "0.0.8",
  "main": "build/index.js",
  "types": "build/index.ts",
  "type": "module",
  "files": [
    "build",
    "README.md"
  ],
  "exports": {
    ".": {
      "import": "./build/index.js",
      "types": "./build/index.d.ts"
    }
  },
  "typesVersions": {
