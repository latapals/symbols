      "*": [
        "./build/*/index.d.ts",
        "./build/index.d.ts"
      ]
    }
  },
  "scripts": {
    "build-icons": "svgo-jsx",
    "build-ts": "tsup",
    "build": "bun run build-icons && bun run build-ts"
  },
  "devDependencies": {
    "@svgo/jsx": "^0.4.2",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "tsup": "^8.2.4",
    "typescript": "^5.6.2"
  },
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18"
  }
}
